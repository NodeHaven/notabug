  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "clean": "rimraf build && rimraf server-build",
    "start": "parcel src/index.html",
    "build": "yarn buildui && yarn buildserver",
    "buildui": "cross-env-shell BABEL_ENV=client \"rimraf build && parcel build -d build src/index.html\" && yarn tohtdocs",
    "tohtdocs": "copyfiles --up 1 build/* build/**/* htdocs",
    "buildserver": "cross-env-shell BABEL_ENV=server \"rimraf server-build && parcel build -d server-build -o index.js -t node --no-minify src/server-peer.js\""
  },
  "alias": {
    "argon2": "./src/vendor/argon2.stub.js"
  },
  "dependencies": {
    "@notabug/gun-receiver": "^0.5.0",
